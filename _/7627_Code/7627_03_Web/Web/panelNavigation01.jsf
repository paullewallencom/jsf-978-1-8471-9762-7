<html xmlns:f="http://java.sun.com/jsf/core" xmlns:t="http://myfaces.apache.org/tomahawk" xmlns:h="http://java.sun.com/jsf/html">
<f:view>
<head>
<meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
<title>t:panelNavigation example</title>
<f:loadBundle var="bundle" basename="messages" />
<link rel="stylesheet" type="text/css" href="css/style.css" />
</head>
<body>
	<h:form>
		<t:panelNavigation>
			<t:commandNavigation value="Home" styleClass="commandNavigation" />
			<t:commandNavigation value="About" styleClass="commandNavigation" />
			<t:commandNavigation value="Products" styleClass="commandNavigation" action="#{productBean.displayProducts}" />
			<t:commandNavigation value="Services" styleClass="commandNavigation" />
			<t:commandNavigation value="Contact Us" styleClass="commandNavigation" />
		</t:panelNavigation>
	</h:form>
</body>
</f:view>
</html>
